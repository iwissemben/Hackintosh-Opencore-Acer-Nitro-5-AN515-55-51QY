<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>ACPI</key>
	<dict>
		<key>Add</key>
		<array>
			<dict>
				<key>Comment</key>
				<string>Replacement DMAR table with Reserved Memory Regions stripped - requires DMAR table be dropped</string>
				<key>Enabled</key>
				<true/>
				<key>Path</key>
				<string>DMAR.aml</string>
			</dict>
		</array>
		<key>Delete</key>
		<array>
			<dict>
				<key>All</key>
				<false/>
				<key>Comment</key>
				<string>Drop DMAR Table</string>
				<key>Enabled</key>
				<true/>
				<key>OemTableId</key>
				<data>
				AAAAAA==
				</data>
				<key>TableLength</key>
				<integer>0</integer>
				<key>TableSignature</key>
				<data>
				RE1BUg==
				</data>
			</dict>
		</array>
	</dict>
</dict>
</plist>
